  object FDqrySaidaProdutos: TFDQuery
    Connection = frmPrincipal.FDconexao
    Transaction = FDtcSaidaProdutos
    UpdateOptions.AssignedValues = [uvFetchGeneratorsPoint, uvGeneratorName]
    UpdateOptions.FetchGeneratorsPoint = gpImmediate
    UpdateOptions.GeneratorName = 'GEN_SAIDA_PRODUTO_ID'
    UpdateOptions.UpdateTableName = 'SAIDA_PRODUTO'
    UpdateOptions.KeyFields = 'SAIDA_PRODUTO_ID'
    UpdateOptions.AutoIncFields = 'SAIDA_PRODUTO_ID'
    SQL.Strings = (
      'select '
      ''
      ' S.SAIDA_PRODUTO_ID,'
      ' S.DESTINO_ID,'
      ' S.PROPRIEDADE_ID,'
      ' S.PRODUTO_ID,'
      ' S.DATA,'
      ' S.QTDE,'
      ' S.VALOR_SAIDA,'
      ' S.OBS,'
      ' D.DESCRICAO as DESTINO,'
      ' P.DESCRICAO as PROPRIEDADE,'
      ' PRO.DESCRICAO as PRODUTO'
      ''
      'from'
      ''
      ' SAIDA_PRODUTO S, DESTINO D, PROPRIEDADE P, PRODUTO PRO'
      ''
      'where'
      ''
      ' (S.DESTINO_ID = D.DESTINO_ID) and'
      ' (S.PROPRIEDADE_ID = P.PROPRIEDADE_ID) and'
      ' (S.PRODUTO_ID = PRO.PRODUTO_ID) and'
      ' (S.DATA between :INICIO and :FIM) '
      ''
      'order by S.SAIDA_PRODUTO_ID')
    Left = 496
    Top = 296
    ParamData = <
      item
        Name = 'INICIO'
        DataType = ftDate
        ParamType = ptInput
        Value = Null
      end
      item
        Name = 'FIM'
        DataType = ftDate
        ParamType = ptInput
      end>
    object FDqrySaidaProdutosSAIDA_PRODUTO_ID: TIntegerField
