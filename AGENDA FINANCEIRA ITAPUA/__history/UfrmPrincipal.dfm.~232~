  object FDqryVencimentos: TFDQuery
    Connection = FDconexao
    FetchOptions.AssignedValues = [evRowsetSize]
    FetchOptions.RowsetSize = 200000
    SQL.Strings = (
      'select * from REGISTRO_DE_GASTOS '
      ''
      'where'
      ''
      '(DATA_VENCIMENTO <= :VENCIMENTO) AND (PAGO = 0)')
    Left = 33
    Top = 160
    ParamData = <
      item
        Name = 'VENCIMENTO'
        DataType = ftString
        ParamType = ptInput
        Value = '05/27/2023'
      end>
  end
