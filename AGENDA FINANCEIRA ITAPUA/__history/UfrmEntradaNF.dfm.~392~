  object FDqryEntradaNF: TFDQuery
    Connection = frmPrincipal.FDconexao
    Transaction = FDtcEntradaNF
    UpdateOptions.AssignedValues = [uvFetchGeneratorsPoint, uvGeneratorName]
    UpdateOptions.GeneratorName = 'GEN_CADASTRO_NF_ID'
    UpdateOptions.UpdateTableName = 'CADASTRO_NF'
    UpdateOptions.KeyFields = 'CADASTRO_NF_ID'
    UpdateOptions.AutoIncFields = 'CADASTRO_NF_ID'
    SQL.Strings = (
      'select '
      '  NF.CADASTRO_NF_ID,'
      '  '#39#39' as ADD_ITEM,'
      '  NF.FORNECEDOR_ID,'
      '  NF.DATA,'
      '  NF.VALOR_NF,'
      '  NF.NF,'
      '  NF.DATA_EMISSAO_NF,'
      '  NF.OBS,'
      '  NF.STATUS,'
      '  NF.NF_LANCADA,'
      '  F.NOME_FANTASIA,'
      '  F.RAZAO_SOCIAL'
      ''
      'from'
      ''
      '  CADASTRO_NF NF, FORNECEDOR F'
      ''
      'where'
      ''
      ' (NF.FORNECEDOR_ID = F.FORNECEDOR_ID) and'
      ' (NF.DATA between :INICIO and :FIM)'
      ''
      ''
      'order by'
      ''
      '  NF.CADASTRO_NF_ID ASC')
    Left = 580
    Top = 251
    ParamData = <
      item
        Name = 'INICIO'
        DataType = ftDate
        ParamType = ptInput
        Value = Null
      end
      item
        Name = 'FIM'
        DataType = ftDate
        ParamType = ptInput
      end>
    object FDqryEntradaNFCADASTRO_NF_ID: TIntegerField
